package com.test.mvc;


import java.util.ArrayList;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import javax.servlet.http.HttpSession;

import org.springframework.web.servlet.ModelAndView;
import org.springframework.web.servlet.mvc.Controller;

public class ClubBeforeOpenedListDControll implements Controller
{
	//주요 속성 구성
	// → 인터페이스 형태의 자료형을 속성으로 구성
	private IClubDAO dao;

	// setter 구성
	public void setDao(IClubDAO dao)
	{
		this.dao = dao;
	}
	
	@Override
	public ModelAndView handleRequest(HttpServletRequest request, HttpServletResponse response) throws Exception
	{
		ModelAndView mav = new ModelAndView();


		preopenDetail preopenDetail = new preopenList();
		
		String title = request.getParameter("title");

		try
		{
			preopenDetail = dao.preopenList(title);

			mav.addObject("bee", );
			mav.addObject("test", );

			mav.setViewName("ClubBeforeOpenedContents.jsp");
		} catch (Exception e)
		{
			System.out.println(e.toString());
		}

		return mav;
	}

}